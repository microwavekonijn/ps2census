{
  "name": "ps2census",
  "version": "3.3.3",
  "description": "Client to connect to the PS2 Event Stream websocket.",
  "main": "dist",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "test": "echo \"No test specified\" && exit 0",
    "test-run": "ts-node ./test/test.ts",
    "prepublishOnly": "npm run build",
    "postversion": "git push && git push --tags",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microwavekonijn/ps2census.git"
  },
  "keywords": [
    "ps2",
    "census",
    "websocket",
    "client",
    "gaming"
  ],
  "author": "Microwavekonijn",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/microwavekonijn/ps2census/issues"
  },
  "homepage": "https://github.com/microwavekonijn/ps2census#readme",
  "dependencies": {
    "axios": "0.27.0",
    "ws": "8.5.0"
  },
  "optionalDependencies": {
    "bufferutil": "4.0.6",
    "utf-8-validate": "5.0.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-angular": "^16.2.3",
    "@types/node": "12.20.49",
    "@types/ws": "8.5.3",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "dotenv": "16.0.0",
    "eslint": "8.14.0",
    "husky": "^7.0.0",
    "lerna-changelog": "^2.2.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "ts-node": "10.7.0",
    "typescript": "4.6.3"
  },
  "lint-staged": {
    "{src,test}/**/*.{ts,json}": [
      "prettier --ignore-path ./.prettierignore --write"
    ]
  }
}
