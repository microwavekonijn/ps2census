{
  "name": "ps2census",
  "version": "4.5.6",
  "description": "Client to connect to the PS2 Event Stream websocket.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "test": "echo \"No test specified\" && exit 0",
    "test-run": "ts-node ./test/test.ts",
    "format": "prettier --write",
    "lint": "eslint src",
    "prepublishOnly": "npm run build",
    "postversion": "git push && git push --tags",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microwavekonijn/ps2census.git"
  },
  "keywords": [
    "planetside",
    "ps2",
    "census",
    "websocket",
    "client",
    "gaming"
  ],
  "author": "Microwavekonijn",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/microwavekonijn/ps2census/issues"
  },
  "homepage": "https://github.com/microwavekonijn/ps2census#readme",
  "dependencies": {
    "axios": "1.4.0",
    "eventemitter3": "5.0.1",
    "isomorphic-ws": "5.0.0",
    "ws": "8.13.0"
  },
  "peerDependencies": {
    "bufferutil": "^4.0.7",
    "utf-8-validate": ">=5.0.10"
  },
  "peerDependenciesMeta": {
    "bufferutil": {
      "optional": true
    },
    "utf-8-validate": {
      "optional": true
    }
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.1",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-angular": "^17.6.6",
    "@types/node": "^20.3.2",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "{src,test}/**/*.{ts,json}": [
      "prettier --ignore-path ./.prettierignore --write"
    ]
  }
}
