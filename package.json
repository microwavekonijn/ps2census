{
  "name": "ps2census",
  "version": "4.5.3",
  "description": "Client to connect to the PS2 Event Stream websocket.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "test": "echo \"No test specified\" && exit 0",
    "test-run": "ts-node ./test/test.ts",
    "format": "prettier --write",
    "lint": "eslint src",
    "changelog": "lerna-changelog",
    "prepublishOnly": "npm run build",
    "postversion": "git push && git push --tags",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microwavekonijn/ps2census.git"
  },
  "keywords": [
    "planetside",
    "ps2",
    "census",
    "websocket",
    "client",
    "gaming"
  ],
  "author": "Microwavekonijn",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/microwavekonijn/ps2census/issues"
  },
  "homepage": "https://github.com/microwavekonijn/ps2census#readme",
  "dependencies": {
    "axios": "1.3.4",
    "eventemitter3": "5.0.0",
    "isomorphic-ws": "5.0.0",
    "ws": "8.12.1"
  },
  "peerDependencies": {
    "bufferutil": "^4.0.7",
    "utf-8-validate": ">=5.0.10"
  },
  "peerDependenciesMeta": {
    "bufferutil": {
      "optional": true
    },
    "utf-8-validate": {
      "optional": true
    }
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-angular": "^17.4.4",
    "@types/node": "^18.14.0",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "{src,test}/**/*.{ts,json}": [
      "prettier --ignore-path ./.prettierignore --write"
    ]
  }
}
